{
	"files.associations": {
		"*.cppm": "cpp",
		"*.d": "makefile",
  		"dockerfile-system": "dockerfile"
	},
	"search.exclude": {
        "**/.git": true,
        "**/lib": true,
        "**/tmp": true,
		"third-party/llvm-project": true,
		"third-party/antlr": true
	},
	"clangd.arguments": [
		"-header-insertion=never"
	]
}
